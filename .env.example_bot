# ==================== 机器人 example_bot 环境配置 ====================
# 请填入实际的配置值

# Telegram Bot配置
BOT_TOKEN=your_bot_token
API_ID=your_api_id
API_HASH=your_api_hash

# Firebase配置
FIREBASE_PROJECT_ID=your_project_id

# 存储模式 (true=本地存储, false=Firebase存储)
USE_LOCAL_STORAGE=true

# 端口配置
PORT=8080

# 部署环境 (true=Render环境, false=本地环境)
IS_RENDER=false
